{
  "name": "code-with-mosh-downloader",
  "version": "0.0.1",
  "description": "Download courses from codewithmosh.com",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-rebuild && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "test": "mocha --require ts-node/register src/**/*.spec.ts"
  },
  "keywords": [],
  "author": "Emily Medhurst",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_test"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-flatpak",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.58",
    "@electron-forge/maker-deb": "^6.0.0-beta.58",
    "@electron-forge/maker-flatpak": "^6.0.0-beta.58",
    "@electron-forge/maker-rpm": "^6.0.0-beta.58",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.58",
    "@electron-forge/plugin-webpack": "6.0.0-beta.58",
    "@electron-forge/publisher-github": "^6.0.0-beta.58",
    "@electron-forge/test-utils": "^6.0.0-beta.58",
    "@types/async-lock": "^1.1.3",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.4.0",
    "@types/node-fetch": "^2.5.11",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@vercel/webpack-asset-relocator-loader": "^1.6.0",
    "async-lock": "^1.3.0",
    "electron": "13.1.7",
    "eslint": "^7.31.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "htmlparser2": "^6.1.0",
    "mocha": "^9.0.2",
    "node-html-parser": "^4.1.0",
    "node-loader": "^2.0.0",
    "react-is": "^17.0.2",
    "spectron": "^15.0.0",
    "style-loader": "^3.2.1",
    "styled-components": "^5.3.0",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.0.2",
    "webpack": "^5.45.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
